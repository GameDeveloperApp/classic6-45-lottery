{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "buy-ticket.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/buy-ticket",
      "dest": "/buy-ticket.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
